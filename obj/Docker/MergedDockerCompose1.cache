services:
  login-service:
    build:
      context: C:\Users\billy\source\repos\spotirightfree
      dockerfile: login-service/login-service/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: login-service
      target: base
    container_name: login-service
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: loginservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/login-service.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\billy\source\repos\spotirightfree\login-service\login-service:/app:rw
    - C:\Users\billy\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\billy\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\billy\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\billy\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\billy\source\repos\spotirightfree:/src:rw
version: '3.4'