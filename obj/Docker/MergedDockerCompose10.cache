services:
  music-service:
    build:
      context: C:\Users\billy\source\repos\spotirightfree
      dockerfile: music-service/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: music-service
      target: base
    container_name: music-service_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: http://+:5088
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: musicservice:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/music-service.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 5088
    tty: true
    volumes:
    - C:\Users\billy\source\repos\spotirightfree\music-service\music-service:/app:rw
    - C:\Users\billy\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\billy\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\billy\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\billy\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\billy\source\repos\spotirightfree:/src:rw
  playlist-service:
    build:
      context: C:\Users\billy\source\repos\spotirightfree
      dockerfile: playlist-service/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: http://+:5049
    image: playlistservice
    ports:
    - target: 5049
    volumes:
    - C:\Users\billy\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\billy\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'